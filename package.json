{
  "name": "sanity-template-sveltekit-clean",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "sveltekit-app",
    "studio"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=sveltekit-app\" \"npm run dev --workspace=studio\"",
    "format": "prettier --cache --write ."
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-packagejson"
    ],
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "prettier": "3.5.3",
    "prettier-plugin-packagejson": "2.5.12"
  },
  "packageManager": "pnpm@10.10.0"
}
